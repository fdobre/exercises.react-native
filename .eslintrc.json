{
  "root": true,
  "extends": [
      "@react-native-community",
      "prettier",
      "prettier/@typescript-eslint",
      "prettier/react"
  ],
  "rules": {
      "prettier/prettier": ["error", {
        "arrowParens": "avoid"
      }],
      "no-unused-vars": "off",
      "@typescript-eslint/no-unused-vars": 0
  }
}

